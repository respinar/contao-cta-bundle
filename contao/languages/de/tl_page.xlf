                <source>CTA url title</source>
                <target>CTA-Linktitel</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaTitle.1">
                <source>Enter the link title for the CTA button or link.</source>
                <target>Geben Sie den Linktitel für die CTA-Schaltfläche oder den Link ein.</target>
            </trans-unit>

            <trans-unit id="tl_page.ctaUrl.0">
                <source>CTA url</source>
                <target>CTA-URL</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaUrl.1">
                <source>Enter the url for CTA.</source>
                <target>Geben Sie die URL für die CTA ein.</target>
            </trans-unit>

            <trans-unit id="tl_page.ctaText.0">
                <source>CTA button text</source>
                <target>CTA-Schaltflächentext</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaText.1">
