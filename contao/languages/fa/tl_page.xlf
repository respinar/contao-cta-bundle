    <file source-language="en" target-language="fa" datatype="plaintext" original="tl_page">
        <body>
            <!-- Legend -->
            <trans-unit id="tl_page.cta_legend">
                <source>CTA button settings</source>
                <target>تنظیمات دکمه اقدام</target>
            </trans-unit>

            <!-- Fields -->
            <trans-unit id="tl_page.ctaVisibility.0">
                <source>Call to Action Visibility</source>
                <target>قابلیت نمایش فراخوان به اقدام (CTA)</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaVisibility.1">
                <source>Define whether the Call to Action should be shown or hidden on this page. Leave it blank to inherit the setting from the parent page or module settings.</source>
                <target>تعیین کنید که آیا عنصر دعوت به اقدام (Call to Action) باید در این صفحه نمایش داده شود یا خیر. برای ارث‌بری از صفحه والد یا تنظیمات ماژول، این فیلد را خالی بگذارید.</target>
            </trans-unit>

            <trans-unit id="tl_page.ctaTitle.0">
                <source>CTA url title</source>
                <target>عنوان لینک CTA</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaTitle.1">
                <source>Enter the link title for the CTA button or link.</source>
                <target>عنوان لینک دکمه CTA یا لینک را وارد کنید.</target>
            </trans-unit>

            <trans-unit id="tl_page.ctaUrl.0">
                <source>CTA url</source>
                <target>آدرس URL CTA</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaUrl.1">
                <source>Enter the url for CTA.</source>
                <target>آدرس URL برای CTA را وارد کنید.</target>
            </trans-unit>

            <trans-unit id="tl_page.ctaText.0">
                <source>CTA button text</source>
                <target>متن دکمه CTA</target>
            </trans-unit>
            <trans-unit id="tl_page.ctaText.1">
                <source>Enter the text of button of CTA.</source>
                <target>متن دکمه CTA را وارد کنید.</target>
            </trans-unit>
        </body>
